                                Height="25"
                                Width="80"
                                Margin="0,100,10,0"                               
                                x:Name="YesButton"
                                Click="YesButton_Click"/>

                        <Button Template="{DynamicResource CancelButtonStyle}"
                                Style="{StaticResource SideButton}"
                                BorderThickness="1"
                                BorderBrush="#AEAEAE"
                                Background="White"
                                Foreground="#FF6A6A6A"
                                Content="Cancel"
                                FontSize="12"
                                Height="25"
                                Width="80"
                                Margin="10,100,0,0"
                                Cursor="Hand"
                                x:Name="NoButton"
                                Click="CancelButton_Click"/>
                    </StackPanel>
                </Grid>
            </Border>
        </Grid>

        <Grid x:Name="RejectBox" Visibility="Collapsed" Panel.ZIndex="100" Grid.ColumnSpan="3" Grid.RowSpan="2">
            <Grid Background="Black" Opacity="0.5"/>
            <Border Style="{StaticResource BorderStyle}"
                    Padding="20"
                    Width="400"
                    Height="200"
                    CornerRadius="8"
                    HorizontalAlignment="Center" 
                    VerticalAlignment="Center">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="50"/>
                    </Grid.RowDefinitions>

                    <ScrollViewer HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                        <ListBox Style="{StaticResource ListBoxStyle}" x:Name="rejectListBox" BorderThickness="0">

                            <ListBox.ItemsPanel>
                                <ItemsPanelTemplate>
                                    <UniformGrid Columns="3" />
                                </ItemsPanelTemplate>
                            </ListBox.ItemsPanel>

                            <ListBox.ItemTemplate>
                                <DataTemplate>
                                    <Control MouseDoubleClick="selectReject_doubleclick" Cursor="Hand">
                                        <Control.Template>
                                            <ControlTemplate>

                                                <Border Style="{StaticResource BorderStyle}" Cursor="Hand" Height="80" Width="100" CornerRadius="8">
                                                    <StackPanel>
                                                        <Image Style="{StaticResource FolderIconStyle}" Width="30" Height="30"/>
                                                        <TextBlock Style="{StaticResource TextBlockStyle}" Text="{Binding rejectFolderName}" FontSize="12" HorizontalAlignment="Center"/>
                                                        <TextBlock Style="{StaticResource TextBlockStyle}" Text="{Binding rejectNum}" FontSize="12" HorizontalAlignment="Center"/>
                                                    </StackPanel>
                                                </Border>

                                            </ControlTemplate>
                                        </Control.Template>
                                    </Control>
                                </DataTemplate>
                            </ListBox.ItemTemplate>

                        </ListBox>
                    </ScrollViewer>

                    <Button Template="{DynamicResource CancelButtonStyle}"
                            Style="{StaticResource SideButton}"
                            BorderThickness="1"
                            BorderBrush="#AEAEAE"
                            Background="White"
                            Foreground="#FF6A6A6A"
                            Content="Cancel"
                            FontSize="12"
                            Height="25"
                            Width="80"
                            Margin="0,20,0,0"
                            Cursor="Hand"
                            Click="cancelRejectList_Click" 
                            Grid.Row="1" 
                            HorizontalAlignment="Center" 
                            VerticalAlignment="Center"/>
                </Grid>
            </Border>
        </Grid>

        <Grid x:Name="SummaryBox" Visibility="Collapsed" Panel.ZIndex="100" Grid.ColumnSpan="3" Grid.RowSpan="2">
            <Grid Background="Black" Opacity="0.5"/>
            <Border Style="{StaticResource BorderStyle4}"
                    Width="500"
                    MaxHeight="750"
                    CornerRadius="8" 
                    HorizontalAlignment="Center" 
                    VerticalAlignment="Center">
                <Grid>
                    <StackPanel>
                        <Border Style="{StaticResource BorderStyle}" Margin="30" CornerRadius="8" Height="220">
                            <Grid>
                                <Label Style="{StaticResource LabelStyle}" Content="Summary" Margin="0,15,0,10" HorizontalAlignment="Center" VerticalAlignment="Top" FontSize="30" FontWeight="Bold"/>

                                <Separator VerticalAlignment="Top" Margin="20,70,20,0"/>

                                <StackPanel Margin="20,80,20,10">
                                    <Label Style="{StaticResource LabelStyle}" Content="Total Image: " HorizontalAlignment="Center"  VerticalAlignment="Center" FontSize="15" FontWeight="DemiBold" x:Name="summary_total"/>
                                    <Label Style="{StaticResource LabelStyle}" Content="Total Inspected Image: " HorizontalAlignment="Center"  VerticalAlignment="Center" FontSize="15" FontWeight="DemiBold" x:Name="summary_totalinspect"/>
                                    <Label Style="{StaticResource LabelStyle}" Content="Pass Count: " HorizontalAlignment="Center"  VerticalAlignment="Center" FontSize="15" FontWeight="DemiBold" x:Name="summary_pass"/>
                                    <Label Style="{StaticResource LabelStyle}" Content="Reject Count: " HorizontalAlignment="Center"  VerticalAlignment="Center" FontSize="15" FontWeight="DemiBold" x:Name="summary_fail"/>
                                </StackPanel>
                            </Grid>
                        </Border>

                        <Border Style="{StaticResource BorderStyle}" Margin="30,0,30,30" CornerRadius="8" MaxHeight="200">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="5*"/>
                                    <RowDefinition Height="5*"/>
                                </Grid.RowDefinitions>

                                <Label Style="{StaticResource LabelStyle}" Content="Reject Folder Breakdown" Margin="0,15,0,10" HorizontalAlignment="Center"  VerticalAlignment="Top" FontSize="25" FontWeight="Bold"/>

                                <ScrollViewer Margin="30,10,30,20" HorizontalAlignment="Stretch" VerticalAlignment="Bottom" Height="75" Grid.Row="1">
                                    <ListBox Style="{StaticResource ListBoxStyle}" x:Name="summaryListBox" BorderThickness="0">
                                        <ListBox.ItemTemplate>
                                            <DataTemplate>
                                                <StackPanel>
                                                    <TextBlock Style="{StaticResource TextBlockStyle}"  FontSize="12" HorizontalAlignment="Center">
                                                        <TextBlock.Text>
                                                            <MultiBinding StringFormat="{}{0} {1}">
                                                                <Binding Path="rejectFolderName" />
                                                                <Binding Path="rejectNum" />
                                                            </MultiBinding>
                                                        </TextBlock.Text>
                                                    </TextBlock>
                                                    <!--<TextBlock Style="{StaticResource TextBlockStyle}" Text="{Binding rejectNum}" FontSize="12" HorizontalAlignment="Center"/> Text="{Binding rejectFolderName}"-->
                                                </StackPanel>
                                            </DataTemplate>
                                        </ListBox.ItemTemplate>
                                    </ListBox>
                                </ScrollViewer>
                            </Grid>
                        </Border>

                        <Button Template="{DynamicResource CancelButtonStyle}"
                                Style="{StaticResource SideButton}"
                                BorderThickness="1"
                                BorderBrush="#AEAEAE"
                                Background="White"
                                Foreground="#FF6A6A6A"
                                Content="Close"
                                FontSize="12"
                                Height="25"
                                Width="80"
                                Margin="0,10,0,10"
                                Cursor="Hand"
                                Click="closeSummary_Click" 
                                HorizontalAlignment="Center" 
                                VerticalAlignment="Center"/>
                    </StackPanel>
                </Grid>
            </Border>
        </Grid>

        <Grid x:Name="ProgressBox" Visibility="Collapsed" Panel.ZIndex="100" Grid.ColumnSpan="3" Grid.RowSpan="2">
            <Grid Background="Black" Opacity="0.5"/>
            <Border Style="{StaticResource BorderStyle}"
                    MinWidth="400"
                    MinHeight="200"
                    CornerRadius="8" 
                    HorizontalAlignment="Center" 
                    VerticalAlignment="Center">
                <Grid>
                    <StackPanel x:Name="ProgressBox_progress" Visibility="Collapsed">
                        <TextBlock Style="{StaticResource TextBlockStyle}" Margin="0,20,0,5" Text="Uploading..." FontWeight="Bold" HorizontalAlignment="Center" VerticalAlignment="Top" x:Name="progressmsg"/>
                        <TextBlock Style="{StaticResource TextBlockStyle}" Margin="0,0,0,10" Text="0/0" FontWeight="Bold" HorizontalAlignment="Center" VerticalAlignment="Top" x:Name="progressnum"/>
                        <ProgressBar x:Name="pbBar" Minimum="0" Maximum="100" Height="10" Margin="10" IsIndeterminate="true"/>
                        <Button Template="{DynamicResource GeneralButtonStyle}"
                                Style="{StaticResource MainButton}" 
                                x:Name="sUploadButton" 
                                BorderThickness="0"
                                Background="#CC9900"
                                Foreground="#FFF1F1F1" 
                                Content="Start" 
                                FontSize="12" 
                                Height="25" 
                                Width="80" 
                                Margin="0,10,0,0"
                                HorizontalAlignment="Center" 
                                VerticalAlignment="Center"
                                Click="sUploadButton_Click" 
                                Visibility="Collapsed"/>
                    </StackPanel>

                    <StackPanel x:Name="ProgressBox_error" Visibility="Collapsed">
                        <StackPanel Orientation="Horizontal">
                            <Image HorizontalAlignment="Left" 
                                   Width="50"
                                   Height="50"
                                   Margin="15,15,0,0">
                                <Image.Source>
                                    <BitmapImage DecodePixelWidth="300" UriSource="/Resources/alert-red.png" />
                                </Image.Source>
                            </Image>
                            <TextBlock Style="{StaticResource TextBlockStyle}" Margin="10,15,0,0" VerticalAlignment="Center" Text="Duplicate image file name found!" FontWeight="Bold" x:Name="progresserrormsg"/>
                        </StackPanel>

                        <TextBlock Style="{StaticResource TextBlockStyle}" Margin="15" HorizontalAlignment="Center" Text="" FontWeight="Bold" x:Name="dupOriTextBlock"/>

                        <TextBox Style="{StaticResource TextBoxStyle2}" MinWidth="200" FontSize="12" HorizontalAlignment="Center" VerticalAlignment="Center" x:Name="progress_InputTextBox"/>

                        <Button Template="{DynamicResource GeneralButtonStyle}"
                                Style="{StaticResource MainButton}" 
                                x:Name="sRenameButton"
                                BorderThickness="0"
                                Background="#CC9900"
                                Foreground="#FFF1F1F1" 
                                Content="Start" 
                                FontSize="12" 
                                Height="25" 
                                Width="80" 
                                Margin="0,10,0,0"
                                HorizontalAlignment="Center" 
                                VerticalAlignment="Center"
                                Click="sRenameButton_Click"
                                Visibility="Collapsed"/>

                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="0,20,0,0">
                            <Button Template="{DynamicResource GeneralButtonStyle}"
                                    Style="{StaticResource MainButton}"  
                                    BorderThickness="0"
                                    Background="#CC9900"
                                    Foreground="#FFF1F1F1" 
                                    x:Name="RenameButton" 
                                    Margin="5" 
                                    Content="Rename" 
                                    FontSize="12" 
                                    Height="30" 
                                    Width="100"
                                    Click="RenameButton_Click"/>
                            <Button 
                                    Template="{DynamicResource GeneralButtonStyle}"
                                    Style="{StaticResource MainButton}"  
                                    BorderThickness="0"
                                    Background="#CC9900"
                                    Foreground="#FFF1F1F1"
                                    FontSize="12" 
                                    Height="30" 
                                    Width="100"
                                    x:Name="QuickRenameButton" 
                                    Margin="5" 
                                    Content="Quick Rename" 
                                    Click="QuickRenameButton_Click"/>
                            <Button Template="{DynamicResource CancelButtonStyle}"
                                    Style="{StaticResource SideButton}"
                                    BorderThickness="1"
                                    BorderBrush="#AEAEAE"
                                    Background="White"
                                    Foreground="#FF6A6A6A"
                                    x:Name="SkipButton" 
                                    Margin="5" 
                                    Content="Skip" 
                                    FontSize="12" 
                                    Height="30" 
                                    Width="100"
                                    Click="SkipButton_Click" />
                            <Button Template="{DynamicResource CancelButtonStyle}"
                                    Style="{StaticResource SideButton}"
                                    BorderThickness="1"
                                    BorderBrush="#AEAEAE"
                                    Background="White"
                                    Foreground="#FF6A6A6A"
                                    x:Name="SkipAllButton" 
                                    Margin="5" 
                                    Content="Skip All"
                                    FontSize="12" 
                                    Height="30" 
                                    Width="100"
                                    Click="SkipAllButton_Click" />
                        </StackPanel>
                    </StackPanel>
                </Grid>
            </Border>
        </Grid>

        <Grid x:Name="ManageRejectBox" Visibility="Collapsed" Panel.ZIndex="100" Grid.ColumnSpan="3" Grid.RowSpan="2">
            <Grid Background="Black" Opacity="0.5"/>
            <Border Style="{StaticResource BorderStyle}"
                    Padding="20"
                    Width="400"
                    MaxHeight="300"
                    CornerRadius="8"
                    HorizontalAlignment="Center" 
                    VerticalAlignment="Center">
                <Grid>

                    <StackPanel x:Name="ManageRejectMenu" Visibility="Collapsed">
                        <TextBlock Style="{StaticResource TextBlockStyle}" Margin="0,20,0,5" Text="Select the folder and choose an option" FontWeight="Bold" HorizontalAlignment="Center" VerticalAlignment="Top" x:Name="manageRejectmsg"/>
                        <ScrollViewer HorizontalAlignment="Stretch" VerticalAlignment="Stretch" MaxHeight="150">
                            <ListBox Style="{StaticResource ListBoxStyle}" x:Name="rejectManageBox" BorderThickness="0">

                                <ListBox.ItemsPanel>
                                    <ItemsPanelTemplate>
                                        <UniformGrid Columns="3" />
                                    </ItemsPanelTemplate>
                                </ListBox.ItemsPanel>

                                <ListBox.ItemTemplate>
                                    <DataTemplate>
                                        <Border Style="{StaticResource BorderStyle}" Cursor="Hand" Height="80" Width="100" CornerRadius="8">
                                            <StackPanel>
                                                <Image Style="{StaticResource FolderIconStyle}" Width="30" Height="30"/>
                                                <TextBlock Style="{StaticResource TextBlockStyle}" Text="{Binding rejectFolderName}" FontSize="12" HorizontalAlignment="Center"/>
                                                <TextBlock Style="{StaticResource TextBlockStyle}" Text="{Binding rejectNum}" FontSize="12" HorizontalAlignment="Center"/>
                                            </StackPanel>
                                        </Border>
                                    </DataTemplate>
                                </ListBox.ItemTemplate>

                            </ListBox>
                        </ScrollViewer>

                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                            <Button Template="{DynamicResource GeneralButtonStyle}"
                                    Style="{StaticResource MainButton}"
                                    Background="#CC9900"
                                    Foreground="#FFF1F1F1"
                                    Content="Rename"
                                    FontSize="12"
                                    Height="25"
                                    Width="80"
                                    Margin="0,20,20,0"
                                    Cursor="Hand"
                                    Click="renameManageReject_Click" 
                                    Grid.Row="1" 
                                    HorizontalAlignment="Center" 
                                    VerticalAlignment="Center"/>

                            <Button Template="{DynamicResource GeneralButtonStyle}"
                                    Style="{StaticResource MainButton}"
                                    Background="#CC9900"
                                    Foreground="#FFF1F1F1"
                                    Content="Delete"
                                    FontSize="12"
                                    Height="25"
                                    Width="80"
                                    Margin="0,20,0,0"
                                    Cursor="Hand"
                                    Click="deleteManageReject_Click" 
                                    Grid.Row="1" 
                                    HorizontalAlignment="Center" 
                                    VerticalAlignment="Center"/>

                            <Button Template="{DynamicResource CancelButtonStyle}"
                                    Style="{StaticResource SideButton}"
                                    BorderThickness="1"
                                    BorderBrush="#AEAEAE"
                                    Background="White"
                                    Foreground="#FF6A6A6A"
                                    Content="Cancel"
                                    FontSize="12"
                                    Height="25"
                                    Width="80"
                                    Margin="20,20,0,0"
                                    Cursor="Hand"
                                    Click="cancelManageReject_Click" 
                                    Grid.Row="1" 
                                    HorizontalAlignment="Center" 
                                    VerticalAlignment="Center"/>
                        </StackPanel>
                    </StackPanel>

                    <StackPanel x:Name="RenameRejectFolder" Visibility="Collapsed">
                        <StackPanel Orientation="Horizontal">
                            <TextBlock Style="{StaticResource TextBlockStyle}" Text="Please enter a new name for " FontWeight="Bold"/>
                            <TextBlock Style="{StaticResource TextBlockStyle}" Text="OriFoldername" x:Name="orifoldername" FontWeight="Bold"/>
                            <TextBlock Style="{StaticResource TextBlockStyle}" Text=":" FontWeight="Bold"/>
                        </StackPanel>
                        <StackPanel HorizontalAlignment="Center">
                            <TextBox Style="{StaticResource TextBoxStyle2}" Width="350" x:Name="newfoldername" Margin="0,10,0,0"/>
                        </StackPanel>



                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                            <Button Content="Rename" Click="btnRenameDefRename_Click"
                                    Template="{DynamicResource GeneralButtonStyle}"
                                    Style="{StaticResource MainButton}"
                                    BorderThickness="0"
                                    Background="#CC9900"
                                    Foreground="#FFF1F1F1"
                                    FontSize="12"
                                    Height="25"
                                    Width="80"
                                    Margin="0,20,20,0"/>
                            <Button Content="Cancel" Click="btnRenameDefCancel_Click"
                                    Template="{DynamicResource CancelButtonStyle}"
                                    Style="{StaticResource SideButton}"
                                    BorderThickness="1"
                                    BorderBrush="#AEAEAE"
                                    Background="White"
                                    Foreground="#FF6A6A6A"
                                    FontSize="12"
                                    Height="25"
                                    Width="80"
                                    Margin="20,20,0,0"
                                    Cursor="Hand"/>
                        </StackPanel>
                    </StackPanel>

                    <StackPanel x:Name="DeleteRejectFolder" Visibility="Collapsed">
                        <TextBlock Style="{StaticResource TextBlockStyle}" Text="Select an option to proceed:" FontWeight="Bold"/>
                        <TextBlock Style="{StaticResource TextBlockStyle}" Text="foldername" x:Name="managedefdelname" FontWeight="Bold"/>
                        <TextBlock Style="{StaticResource TextBlockStyle}" Text="All images in the folder will be deleted" FontWeight="Bold"/>

                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                            <Button Content="Delete all" Click="btnDeleteDefDel_Click"
                                    Template="{DynamicResource GeneralButtonStyle}"
                                    Style="{StaticResource MainButton}"
                                    BorderThickness="0"
                                    Background="#EB212E"
                                    Foreground="#FFF1F1F1"
                                    FontSize="12"
                                    Height="25"
                                    Width="80"
                                    Margin="0,10,10,0"/>
                            <Button Content="Cancel" Click="btnDeleteDefCancel_Click"
                                    Template="{DynamicResource CancelButtonStyle}"
                                    Style="{StaticResource SideButton}"
                                    BorderThickness="1"
                                    BorderBrush="#AEAEAE"
                                    Background="White"
                                    Foreground="#FF6A6A6A"
                                    FontSize="12"
                                    Height="25"
                                    Width="80"
                                    Margin="10,10,0,0"
                                    Cursor="Hand"/>
                        </StackPanel>


                    </StackPanel>
                </Grid>
            </Border>
        </Grid>

    </Grid>
</Window>